{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2021",
    "lib": ["ES2021", "DOM"],
    "esModuleInterop": true,
    "declaration": true,
    "outDir": "./dist",
    "sourceMap": true,
    "newLine": "LF",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "incremental": true,
    "strict": true,
    "downlevelIteration": true,
    "jsx": "react-jsx",
    "noFallthroughCasesInSwitch": true
  },
  "include": ["src"]
}
